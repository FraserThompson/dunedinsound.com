{
  "name": "dunedinsound",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "devDependencies": {
    "command-line-args": "^4.0.7",
    "fs": "0.0.2",
    "glob": "^7.1.1",
    "mkdirp": "^0.5.1",
    "sharp": "^0.18.4",
    "sync-glob": "^1.3.8",
    "yamljs": "^0.2.8"
  },
  "scripts": {
    "serve": "jekyll serve",
    "dev": "jekyll serve --limit_posts=1 --incremental",
    "generate": "node ./_scripts/generate_gig.js",
    "audio": "docker-compose up --build && copy ./_originals/audio/**/*.{mp3,json} ./assets/audio && sync-glob assets/audio/** _site/assets/audio",
    "images": "node ./_scripts/images.js && node ./_scripts/index-media.js && sync-glob assets/img/** _site/assets/img",
    "build": "set JEKYLL_ENV=production && jekyll build",
    "deploy": "s3_website push",
    "production-build": "set JEKYLL_ENV=production && jekyll build && sync-glob assets/img/** _site/assets/img && sync-glob assets/audio/** _site/assets/audio",
    "code-deploy": "set JEKYLL_ENV=production && jekyll build && s3_website push",
    "gig-deploy": "set JEKYLL_ENV=production && jekyll build && sync-glob assets/img/** _site/assets/img && sync-glob assets/audio/** _site/assets/audio && s3_website push",
    "sync": "sync-glob assets/img/** _site/assets/img && sync-glob assets/audio/** _site/assets/audio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FraserThompson/FraserThompson.github.io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FraserThompson/FraserThompson.github.io/issues"
  },
  "homepage": "https://github.com/FraserThompson/FraserThompson.github.io#readme",
  "dependencies": {
    "wavesurfer.js": "^2.0.3"
  }
}
