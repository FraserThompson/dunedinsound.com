    "gatsby-plugin-typography": "^3.10.0",
    "gatsby-remark-copy-linked-files": "^4.7.0",
    "gatsby-remark-images": "^5.7.0",
    "gatsby-remark-smartypants": "^4.7.0",
    "gatsby-source-filesystem": "^3.10.0",
    "gatsby-transformer-remark": "^4.7.0",
    "gatsby-transformer-sharp": "3.10.0",
    "gumshoejs": "^5.1.2",
    "history": "^5.0.0",
    "leaflet": "^1.7.1",
    "polished": "^4.1.3",
    "prismjs": "^1.24.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-image-lightbox": "^5.1.4",
    "react-infinite-scroller": "^1.2.4",
    "react-leaflet": "^3.2.1",
    "react-scrollspy": "^3.4.3",
    "react-transition-group": "^4.4.2",
    "react-typography": "^0.16.20",
    "react-youtube": "^7.13.1",
    "shufflejs": "^5.4.1",
    "srcset": "^4.0.0",
    "throttle-debounce": "^3.0.1",
